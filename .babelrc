{
  "presets": [
    ["@babel/preset-env", {
      "targets": {
        "browsers":["ie > 10","Chrome > 46"]
      },
      "modules": "false",
      "loose":true,
      "forceAllTransforms":true
    }]
  ],
  "plugins": ["@babel/plugin-transform-runtime"],
  "comments": true,
  "env": {
    "test": {
      "presets": ["@babel/preset-env"],
      "plugins": [ "istanbul" ]
    }
  }
}
